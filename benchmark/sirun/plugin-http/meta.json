{
  "name": "plugin-http",
  "setup": "bash -c \"nohup node server.js &\"",
  "run": "node client.js",
  "variants": {
    "without-tracers": {
      "env": {
        "SERVER_USE_TRACER": "0",
        "CLIENT_USE_TRACER": "0",
        "COUNT": "5000"
      }
    },
    "with-server-tracer": {
      "env": {
        "SERVER_USE_TRACER": "1",
        "CLIENT_USE_TRACER": "0",
        "COUNT": "5000"
      }
    },
    "with-client-tracer": {
      "env": {
        "SERVER_USE_TRACER": "0",
        "CLIENT_USE_TRACER": "1",
        "COUNT": "5000"
      }
    },
    "with-both-tracers": {
      "env": {
        "SERVER_USE_TRACER": "1",
        "CLIENT_USE_TRACER": "1",
        "COUNT": "5000"
      }
    }
  }
}
