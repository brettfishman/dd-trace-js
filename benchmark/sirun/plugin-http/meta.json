{
  "name": "plugin-http",
  "variants": {
    "client-control": {
      "setup": "bash -c \"nohup node server.js &\"",
      "run": "node client.js",
      "env": {
        "CLIENT_USE_TRACER": "0",
        "COUNT": "5000"
      }
    },
    "client-with-tracer": {
      "setup": "bash -c \"nohup node server.js &\"",
      "run": "node client.js",
      "env": {
        "CLIENT_USE_TRACER": "1",
        "COUNT": "5000"
      }
    },
    "server-control": {
      "setup": "bash -c \"sleep 2 && nohup node client.js &\"",
      "run": "node server.js",
      "env": {
        "SERVER_USE_TRACER": "0",
        "COUNT": "5000"
      }
    },
    "server-with-tracer": {
      "setup": "bash -c \"sleep 2 && nohup node client.js &\"",
      "run": "node server.js",
      "env": {
        "SERVER_USE_TRACER": "1",
        "COUNT": "5000"
      }
    }
  }
}
